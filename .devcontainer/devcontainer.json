{
	"name": "Go",
	"dockerComposeFile": ["../docker-compose.yml"],
	"service": "dev",
	"settings": {
		"go.toolsManagement.checkForUpdates": "local",
		"go.useLanguageServer": true,
		"go.gopath": "/go",
		"go.goroot": "/usr/local/go"
	},
	"extensions": [
		"golang.Go"
	],
	"remoteUser": "vscode",
	"workspaceFolder": "/workspace"
}